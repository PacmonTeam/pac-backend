generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Status {
  ACTIVE
  INACTIVE
}

model Template {
  id            Int    @id @default(autoincrement())
  script        String
  configuration String
  sequence      Int    @unique
  status        Status @default(ACTIVE)
}
